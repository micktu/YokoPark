// Place your settings in this file to overwrite default and user settings.
{
  "editor.tabSize": 2,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/*.pyc": true
  },
  "standard.enable": true,
  "standard.options": {
    "ignore": [
      "node_modules/**"
    ],
    "globals": ["PIXI", "Howl", "YokoPark", "Data", "TextureData", "getStaticPath", "getImagePath"]
  }
}
